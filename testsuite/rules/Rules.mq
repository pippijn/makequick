		and now it is back
	}

	# Rule without dependency
	include/bar.c: {
		./datagen $@
	}

	# Simple pattern rule
	%.o: %.c {
		$(CC) $< -o $@
	}

	# Rule with many dependencies
	%.o: %.c a b c d e {
		$(CC) $< -o $@
	}
}
